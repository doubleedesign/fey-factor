{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run relay && vite",
		"merge-schema": "cat ../server/src/generated/queryType.graphql ../server/src/generated/typeDefs.graphql > ./src/schema.graphql",
		"relay": "npm run merge-schema && relay-compiler",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"relay": {
		"src": "./src",
		"schema": "./src/schema.graphql",
		"artifactDirectory": "./src/__generated__",
		"language": "typescript",
		"eagerEsModules": true
	},
	"dependencies": {
		"@mdx-js/mdx": "^3.0.1",
		"@mdx-js/react": "^3.0.1",
		"@mdx-js/rollup": "^3.0.1",
		"@types/d3": "^7.4.3",
		"@types/jest": "^29.5.12",
		"@types/react-cytoscapejs": "^1.2.5",
		"@types/react-relay": "^16.0.6",
		"@types/styled-components": "^5.1.34",
		"@upsetjs/react": "^1.11.0",
		"case": "^1.6.3",
		"cytoscape": "^3.30.2",
		"d3": "^7.9.0",
		"fmin": "^0.0.4",
		"lodash": "^4.17.21",
		"polished": "^4.3.1",
		"react": "^18.3.1",
		"react-cytoscapejs": "^2.0.0",
		"react-dom": "^18.3.1",
		"react-loading-skeleton": "^3.5.0",
		"react-relay": "^18.2.0",
		"react-router-dom": "^6.26.1",
		"react-select": "^5.8.0",
		"react-tooltip": "^5.28.0",
		"reaviz": "^15.18.5",
		"relay-runtime": "^18.2.0",
		"remark-gfm": "^4.0.0",
		"styled-components": "^6.1.13"
	},
	"devDependencies": {
		"@doubleedesign/styled-media-queries": "^1.0.0",
		"@eslint/js": "^9.9.0",
		"@stylistic/eslint-plugin-ts": "^2.6.5",
		"@types/lodash": "^4.17.7",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/relay-runtime": "^18.1.1",
		"@typescript-eslint/eslint-plugin": "^8.2.0",
		"@typescript-eslint/parser": "^8.2.0",
		"@vitejs/plugin-react": "^4.3.1",
		"eslint": "^9.9.0",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.9",
		"globals": "^15.9.0",
		"jest": "^29.7.0",
		"jest-cli": "^29.7.0",
		"relay-compiler": "^18.2.0",
		"ts-jest": "^29.1.3",
		"typescript": "^5.5.3",
		"vite": "^5.4.2",
		"vite-plugin-relay": "^2.1.0"
	}
}
